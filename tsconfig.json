  "compilerOptions": {
    "allowJs": true,
    "target": "ES2021",
    "jsx": "react-jsx",
    "noImplicitAny": true,
    "module": "ESNext",
    "strict": true,
    "lib": ["dom"]
  },
  "include": ["./**/*"],
  "exclude": [
    "./plugins/**/*",
    "./typings/**/*",
    "./built/**/*"
  ]
}
